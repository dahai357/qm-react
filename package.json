        "file-loader": "^2.0.0",
        "html-webpack-plugin": "^3.2.0",
        "immutable": "^4.0.0-rc.12",
        "less": "^3.8.1",
        "less-loader": "^4.1.0",
        "postcss-loader": "^3.0.0",
        "style-loader": "^0.23.1",
        "url-loader": "^1.1.2",
